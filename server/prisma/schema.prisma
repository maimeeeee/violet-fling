datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id    Int     @id @default(autoincrement())
  label String
  done  Boolean @default(false)
}

model FileInfo {
  id          Int        @id @default(autoincrement())
  contentname String
  deleted     Boolean    @default(false)
  manage      FileManage @relation(fields: [manageid],references: [id])
  manageid    Int
}

model FileManage{
  id       Int           @id @default(autoincrement())
  parentid Int
  position Int
  depth    Int
  fileinfo FileInfo[]
}

